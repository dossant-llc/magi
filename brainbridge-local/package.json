{
  "name": "brainbridge-local",
  "version": "1.0.0",
  "description": "Local development environment for BrainBridge multi-brain network",
  "main": "launcher.js",
  "scripts": {
    "start": "node launcher.js",
    "dashboard": "node dashboard.js",
    "dev": "concurrently \"npm run start\" \"sleep 5 && npm run dashboard\"",
    "stop": "pkill -f 'node.*brainbridge'",
    "logs": "tail -f logs/*.log",
    "logs:viewer": "node logs-viewer.js",
    "clean": "rm -rf logs/*.log"
  },
  "keywords": ["brainbridge", "mcp", "ai", "local", "development"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}